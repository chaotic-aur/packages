for key in ${!optdepends[@]}; do
  if [[ "${optdepends[${key}]}" =~ ^texlive-most:.*$ ]]; then
    unset optdepends[${key}]
    break
  fi
done

_bump_pkgrel() {
  local _pkgver_old="${1%-*}"
  local _pkgrel_old="${1##*-}"
  local _bump="${2}"

  if [[ "$pkgver" == "$_pkgver_old" ]] && [[ "$pkgrel" == "$_pkgrel_old" ]]; then
    pkgrel+=".$_bump"
  fi
}

_bump_pkgrel 24.06.3-2 2

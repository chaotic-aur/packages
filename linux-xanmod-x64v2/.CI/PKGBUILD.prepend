export MAKEFLAGS="${MAKEFLAGS:-} -j $(nproc)"

# package options
_microarchitecture=92
_config=config_x86-64-v2

use_numa=y
use_tracers=n
#_compiler=clang

_compress_modules=y
_localmodcfg=n
_makenconfig=n

# pkgbase extension
_pkgtype="-x64${_config#config_x86-64-}"
_pkgtype=${_compiler:+-$_compiler}${_pkgtype#-x64v1}

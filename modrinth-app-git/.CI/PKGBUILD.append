makedepends=(${makedepends[@]//java-environment*/java-environment<=24})

_pkgsrc="code"

pkgver() {
  cd "$_pkgsrc"
  git describe --long --tags --abbrev=7 --exclude='*[a-zA-Z][a-zA-Z]*' \
    | sed -E 's/^[^0-9]*//;s/([^-]*-g)/r\1/;s/-/./g'
}

eval _orig_"$(declare -f prepare)"
eval _orig_"$(declare -f build)"

unset -f prepare

build() {
  (_orig_prepare)

  rm -rf "$_pkgsrc/.cargo"

  (_orig_build)
}
